{
  "name": "holly_react",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://lukemcdonald.github.io/holly-react",
  "scripts": {
    "build-css": "node-sass-chokidar src/assets/css/scss/style.scss -o src/assets/css",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "deploy": "npm run build && gh-pages -d build",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "watch-css": "npm run build-css && node-sass-chokidar src/assets/css/scss -o src/assets/css --watch --recursive"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "bootstrap": "^4.5.3",
    "classnames": "^2.2.6",
    "cors": "^2.8.5",
    "material-table": "^1.69.1",
    "mdbreact": "^4.27.0",
    "node-sass-chokidar": "^1.5.0",
    "react": "^16.14.0",
    "react-bootstrap": "^1.4.0",
    "react-cookie": "^4.0.3",
    "react-cookies": "^0.1.1",
    "react-dom": "^16.14.0",
    "react-native-element": "^1.2.1",
    "react-native-elements": "^3.0.0-alpha.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-select": "^3.1.0",
    "react-select-search": "^2.2.0",
    "react-spinners": "^0.9.0",
    "reactjs-popup": "^2.0.4",
    "reactstrap": "^8.7.0",
    "scrollreveal": "^4.0.7",
    "shards-react": "^1.0.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.12.5",
    "gh-pages": "^3.1.0",
    "npm-run-all": "^4.1.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
